		<div class="col-xs-4 col-centered">
			<%= image_tag "video_help_logo.png", :class => "brand-logo img-responsive" %>
		</div>
	</div>
</div>
<div class="row">
	<div class="col-xs-12 col-md-6 col-sm-8 col-centered">
		<div class="youtube-wrap">
